event clicked;/***************************************************************************************
NAME: 			save settings click

ACCESS:			public
		
ARGUMENTS: 		
							none

RETURNS:			none
	
DESCRIPTION:  Saves user properties for all the objects of the current window.
	


//\\//\\//\\//\\//\\//\\//\\//\\//\\//\\//\\//\\//\\//\\//\\//\\//\\//\\//\\//\\//\\//\\
REVISION HISTORY : Created By Dan 9-19-05
	

***************************************************************************************/

n_cst_bso_dynamicobjectmanager lnv_manager

IF parentwindow.triggerEvent( "ue_hasIds" )  = 1 THEN
	parentwindow.dynamic of_getdynpropmansrvc( lnv_manager )

END IF

IF isValid( lnv_manager ) THEN
	IF "Window Template" <> string(parentWindow.dynamic of_getObjName()) THEN
		lnv_manager.of_savesettings()
	ELSE
		MessageBox("Save User Settings", "Cannot save user settings on templates", exclamation!)
	END IF
ELSE
	MessageBox("Save Settings", "Error saving user settings")
END IF
end event

