on m_current.create
call super::create
this.m_administratormode=create m_administratormode
this.m_saveusersettings=create m_saveusersettings
this.m_dash=create m_dash
this.m_revertusersettings=create m_revertusersettings
this.m_refresh=create m_refresh
this.Item[UpperBound(this.Item)+1]=this.m_administratormode
this.Item[UpperBound(this.Item)+1]=this.m_saveusersettings
this.Item[UpperBound(this.Item)+1]=this.m_dash
this.Item[UpperBound(this.Item)+1]=this.m_revertusersettings
this.Item[UpperBound(this.Item)+1]=this.m_refresh
end on

on m_current.destroy
call super::destroy
destroy(this.m_administratormode)
destroy(this.m_saveusersettings)
destroy(this.m_dash)
destroy(this.m_revertusersettings)
destroy(this.m_refresh)
end on

