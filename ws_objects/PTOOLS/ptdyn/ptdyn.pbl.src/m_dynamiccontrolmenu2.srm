type m_revertusersettings from menu within m_current
end type

event clicked;//The following closes the currently active window, and opens a new one with the window
//definition settings.
int 		li_result
String	ls_name


n_cst_bso_dynamicobjectmanager lnv_manager			//reference to the outerwindows property manager.

IF parentwindow.triggerEvent( "ue_hasIds" )  = 1 THEN
	parentwindow.dynamic of_getdynpropmansrvc( lnv_manager )
	ls_name = parentWindow.dynamic of_getObjName()
END IF


IF isValid( lnv_manager ) THEN
	
	IF ls_name <> "Window Template" THEN
		li_result = messageBox("Revert Settings","Reverting user settings will lose any unsaved data, Continue?", Question!, yesno! )
	
		IF li_result = 1 THEN
			lnv_manager.of_loadWindowDefinition()
		
			close(parentwindow)
			MessageBox("Revert Settings", "To go back to old user settings, close this window and relaunch it.")
		END IF
	ELSE
		MessageBox("Revert Settings", "Cannot Revert Settings on a Template, just relaunch Window Template." )
	END IF
ELSE 
	MessageBox("Revert Settings", "Error: Reverting settings")
END IF
end event

on m_revertusersettings.create
call super::create
this.text = "Revert User Settings"
end on

on m_revertusersettings.destroy
call super::destroy
end on

